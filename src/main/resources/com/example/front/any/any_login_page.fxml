<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.front.Controller.any.AnyLoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <TextField text="email" id="email" fx:id="email"/>
    <TextField text="pass" id="pass" fx:id="pass"/>
    <Button text="login" onAction="#onLoginButton"/>
    <Button text="register" onAction="#onRegisterButtonClick"/>


</VBox>
